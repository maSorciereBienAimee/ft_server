sudo docker ps
sudo docker ps -a
sudo docker rm -f ID_CONTAINER
sudo docker images
sudo docker rmi ID_IMAGE
sudo docker exec -i ID_CONTAINER bash -c "ls"
sudo docker run -it --entrypoint /bin/bash ID_IMAGE
sudo docker run -d -p 80:80 ID_IMAGE

